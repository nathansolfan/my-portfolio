{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/auth",
      "dest": "/backend/public/auth.php"
    },
    {
      "src": "/localindex",
      "dest": "/backend/public/localindex.php"
    },
    {
      "src": "/phpinfo",
      "dest": "/backend/public/phpinfo.php"
    },
    {
      "src": "/test_env",
      "dest": "/backend/public/test_env.php"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

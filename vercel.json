{
  "version": 2,
  "builds": [
    {
      "src": "backend/public/index.php",
      "use": "vercel/php"
    },
    {
      "src": "backend/public/auth.php",
      "use": "vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/auth",
      "dest": "/backend/public/auth.php"
    },
    {
      "src": "/(.*)",
      "dest": "/backend/public/index.php"
    }
  ]
}

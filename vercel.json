{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/**/*.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/api/auth",
      "dest": "/api/auth.php"
    },
    {
      "src": "/api/localindex",
      "dest": "/api/localindex.php"
    },
    {
      "src": "/api/phpinfo",
      "dest": "/api/phpinfo.php"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
